<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleTimerWidget">
        <!-- Widget color       -->
        <attr name="widget_color" format="color" />
        <!-- Circle width       -->
        <attr name="circle_width" format="dimension" />
        <!-- Misfires count     -->
        <attr name="separates_count" format="integer" />
        <!-- Length of misfire  -->
        <attr name="separator_length" format="dimension" />
        <!-- Indicator drawable -->
        <attr name="indicator_drawable" format="reference" />
    </declare-styleable>

</resources>